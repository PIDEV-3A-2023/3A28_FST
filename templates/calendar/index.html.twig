



{% block title %}{% endblock %}


<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.11.0/main.min.css" integrity="sha256-5veQuRbWaECuYxwap/IOE/DAwNxgm4ikX7nrgsqYp88=" crossorigin="anonymous">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">



{% block stylesheets %}



    <style>
        #calendrier{
            width:80%;
            margin:auto;
        }

    </style>

{% endblock %}
{% block body %}
<center>
	<a class="btn btn-success" href="{{ path('app_evenement') }}"style="background-color: #2db6fa;border: 1px solid #2db6fa;">Events</a>

</center>


<div id="calendrier">


</div>


{% endblock %}
<script src="https://cdn.jsdelivr.net/npm/fullcalendar@5.11.0/main.min.js" integrity="sha256-XCdgoNaBjzkUaEJiauEq+85q/xi/2D4NcB3ZHwAapoM=" crossorigin="anonymous"></script>



{% block javascripts %}



    <script>
        window.onload=()=>{
  let calendarElt=document.querySelector("#calendrier")
  let calendar= new FullCalendar.Calendar(calendarElt,{
    initialView: 'dayGridMonth',
    locale:'fr',
    headerToolbar:{
        start:'prev,next today',
        center:'title',
        end:'dayGridMonth,timeGridWeek'
    },
    events: JSON.parse('{{ data|raw }}')
  })
  calendar.render()
console.log({{ data|raw }});

}
    </script>
{% endblock %}
